{
    "type": "project",
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        {
            "type": "package",
            "package": [
                {
                    "name": "ibericode/mc4wp-premium",
                    "version": "dev-master",
                    "type": "wordpress-plugin",
                    "dist": {
                        "type": "path",
                        "url": "app/plugins/mc4wp-premium"
                    }
                },
                {
                    "name": "ibericode/html-forms-premium",
                    "version": "dev-master",
                    "type": "wordpress-plugin",
                    "dist": {
                        "type": "path",
                        "url": "app/plugins/html-forms-premium"
                    }
                }
            ]
        },
        {
            "type": "path",
            "url": "app/plugins/koko-analytics"
        },
        {
            "type": "path",
            "url": "app/plugins/mailchimp-for-wordpress"
        },
        {
            "type": "path",
            "url": "app/plugins/mailchimp-top-bar"
        },
        {
            "type": "path",
            "url": "app/plugins/mailchimp-top-bar"
        },
        {
            "type": "path",
            "url": "app/plugins/html-forms"
        },
        {
            "type": "path",
            "url": "app/plugins/wp-snippets"
        }
    ],
    "require": {
        "wp-cli/wp-cli-bundle": "*",
        "roots/wordpress": "^5.0",
        "composer/installers": "^1.9",
        "ibericode/mc4wp-premium": "dev-master",
        "wpackagist-plugin/query-monitor": "^3.5",
        "ibericode/koko-analytics": "dev-master@dev",
        "ibericode/mailchimp-for-wordpress": "dev-master",
        "dannyvankooten/wp-mailchimp-top-bar": "dev-master",
        "ibericode/html-forms": "dev-master",
        "ibericode/html-forms-premium": "dev-master",
        "wpackagist-plugin/woocommerce": "^4.1",
        "luuk/wp-snippets": "dev-master",
        "vlucas/phpdotenv": "^4.1",
        "wpackagist-plugin/mailpoet": "^3.47"
    },
    "extra": {
        "wordpress-install-dir": "www",
        "installer-paths": {
            "www/wp-content/themes/{$name}": [
                "type:wordpress-theme"
            ],
            "www/wp-content/plugins/{$name}": [
                "type:wordpress-plugin"
            ]
        }
    },
    "require-dev": {
        "symfony/var-dumper": "^5.1"
    }
}
